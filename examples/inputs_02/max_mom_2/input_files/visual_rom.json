{
  "path_to_lib": "{{paths.library}}",
  "env": {
    "backend": "numpy",
    "device": "cpu",
    "device_idx": 0,
    "nb_threads": 4,
    "epsilon": null,
    "floatx": "float64",
    "seed": 0
  },
  "system": {
    "name": "BoxAd",
    "init": {
      "species": {
        "Ar": "{{paths.database}}/species/Ar.json",
        "Arp": "{{paths.database}}/species/Arp.json",
        "em": "{{paths.database}}/species/em.json"
      },
      "kin_dtb": "{{paths.database}}/rates/kin_fit.p",
      "rad_dtb": "{{paths.database}}/rates/rad_fit.p",
      "use_rad": true,
      "use_factorial": true
    }
  },
  "models": {
    "cobras": {
      "active": true,
      "name": "CoBRAS",
      "basis": "{{paths.saving}}/max_mom_2/models/cobras/basis_varimax.p"
    },
    "pod": {
      "active": true,
      "name": "POD",
      "basis": "{{paths.saving}}/max_mom_2/models/pod/basis_varimax.p"
    }
  },
  "paths": {
    "saving": "{{paths.saving}}/max_mom_2/"
  },
  "data": {
    "cases": [
      "lrho_lT",
      "lrho_hT",
      "hrho_lT",
      "hrho_hT",
      "mrho_mT"
    ],
    "path": "{{paths.saving}}/data/test/",
    "eps": 1e-08,
    "tout": 0.0,
    "tlim": [
      0.0
    ]
  },
  "rom_range": [
    7,
    11
  ],
  "mpl_style": "{{paths.mpl_style}}",
  "plot": {
    "labels": {
      "Th": "T_h",
      "Te": "T_e",
      "Ar": "\\text{Ar}",
      "Arp": "\\text{Ar$^+$}",
      "em": "\\text{e$^-$}"
    },
    "tlim": {
      "lrho_lT": null,
      "lrho_hT": null,
      "hrho_lT": null,
      "hrho_hT": null,
      "mrho_mT": null
    },
    "teval": {
      "lrho_lT": [
        1e-09,
        1e-08,
        1e-07,
        1e-06,
        1e-05,
        0.0001
      ],
      "lrho_hT": [
        1e-09,
        1e-08,
        1e-07,
        1e-06,
        1e-05,
        0.0001
      ],
      "hrho_lT": [
        1e-09,
        1e-08,
        1e-07,
        1e-06,
        1e-05,
        0.0001
      ],
      "hrho_hT": [
        1e-09,
        1e-08,
        1e-07,
        1e-06,
        1e-05,
        0.0001
      ],
      "mrho_mT": [
        1e-09,
        1e-08,
        1e-07,
        1e-06,
        1e-05,
        0.0001
      ]
    },
    "ylim_err": null,
    "err_scale": "log",
    "hline": {
      "temp": 1.0,
      "mom": 1.0,
      "dist": 10.0
    },
    "max_mom": 2,
    "animate": false,
    "markersize": 6
  }
}