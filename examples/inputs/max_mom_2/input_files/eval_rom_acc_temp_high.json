{
  "path_to_lib": "{{paths.library}}",
  "env": {
    "backend": "numpy",
    "device": "cpu",
    "device_idx": 0,
    "nb_threads": 4,
    "epsilon": null,
    "floatx": "float64",
    "seed": 0
  },
  "system": {
    "name": "BoxAd",
    "init": {
      "species": {
        "Ar": "{{paths.database}}/species/Ar.json",
        "Arp": "{{paths.database}}/species/Arp.json",
        "em": "{{paths.database}}/species/em.json"
      },
      "kin_dtb": "{{paths.database}}/rates/kin_fit.p",
      "rad_dtb": "{{paths.database}}/rates/rad_fit.p",
      "use_rad": true,
      "use_factorial": true
    }
  },
  "models": {
    "cobras": {
      "active": true,
      "name": "CoBRAS",
      "basis": "{{paths.saving}}/max_mom_2/models/cobras/basis.p"
    },
    "cobras_varimax": {
      "active": true,
      "name": "CoBRAS-Varimax",
      "basis": "{{paths.saving}}/max_mom_2/models/cobras/basis_varimax.p"
    },
    "pod": {
      "active": true,
      "name": "POD",
      "basis": "{{paths.saving}}/max_mom_2/models/pod/basis.p"
    },
    "pod_varimax": {
      "active": true,
      "name": "POD-Varimax",
      "basis": "{{paths.saving}}/max_mom_2/models/pod/basis_varimax.p"
    }
  },
  "paths": {
    "saving": "{{paths.saving}}/max_mom_2/error_temp_high/"
  },
  "data": {
    "path": "{{paths.saving}}/data/temp_high/test/",
    "irange": [
      0,
      50
    ],
    "nb_workers": 15,
    "eps": 1e-08,
    "tlim": [
      0.0,
      0.001
    ],
    "tout": 100.0
  },
  "rom_range": [
    8,
    11
  ],
  "mpl_style": "{{paths.mpl_style}}",
  "plot": {
    "active": false,
    "kwargs": {
      "labels": {
        "Th": "T_{\\mathrm{h}}",
        "Te": "T_{\\mathrm{e}}",
        "Ar": "{\\mathrm{Ar}}",
        "Arp": "{\\mathrm{Ar}^+}",
        "em": "{\\mathrm{e}}"
      },
      "tlim": null,
      "ylim_err": {
        "dist": [8e-2, 2e2],
        "temp": {
          "Th": [1e-6, 2e0],
          "Te": [1e-3, 2e1]
        },
        "mom": {
          "Ar": {
            "m0": [1e-6, 2e0],
            "m1": [1e-2, 1e1]
          },
          "Arp": {
            "m0": [1e-2, 2e2],
            "m1": [1e-3, 1e1]
          },
          "em": {
            "m0": [1e-2, 2e2],
            "m1": [1e-1, 1e1]
          }
        }
      },
      "err_scale": "log",
      "hline": {
        "temp": 1.0,
        "mom": 1.0,
        "dist": 10.0
      },
      "max_mom": 2
    }
  }
}